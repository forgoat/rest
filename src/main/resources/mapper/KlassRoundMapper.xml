<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rest.mapper.KlassRoundMapper">
    <select id="findByKlassId" resultType="com.rest.po.KlassRound" parameterType="long">
        SELECT * FROM standard_seminar_system.klass_round WHERE klass_id=#{param1}
    </select>
    <delete id="deleteByRoundId" parameterType="long">
        DELETE FROM standard_seminar_system.klass_round WHERE round_id=#{roundId}
    </delete>
    <select id="findByRoundIdAndClassId" resultType="integer">
        SELECT enroll_number FROM standard_seminar_system.klass_round WHERE round_id=#{param1} AND klass_id=#{param2}
    </select>
    <insert id="save" parameterType="com.rest.po.KlassRound">
        INSERT INTO standard_seminar_system.klass_round(klass_id,round_id,enroll_number) VALUE (#{klassId},#{roundId},#{enrollNumber})
    </insert>
</mapper>