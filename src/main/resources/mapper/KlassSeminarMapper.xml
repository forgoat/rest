<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rest.dao.KlassSeminarDao">
    <select id="findBySeminar" parameterType="long" resultType="com.rest.entity.Klass_seminar">
        SELECT * FROM standard_seminar_system.klass_seminar WHERE seminar_id=#{seminar_id}
    </select>
</mapper>